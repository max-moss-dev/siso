{
  "name": "siso",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start-react": "react-scripts start",
    "start-python": "python main.py",
    "start": "concurrently \"npm run start-python\" \"npm run start-react\"",
    "electron": "wait-on http://localhost:3000 && ELECTRON_START_URL=http://localhost:3000 electron .",
    "dev": "concurrently \"npm run start-python\" \"npm run start-react\" \"npm run electron\"",
    "build-react": "react-scripts build",
    "build-electron": "electron-builder",
    "build": "npm run build-react && npm run build-electron"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "axios": "^1.6.2",
    "diff": "^5.1.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-markdown": "^9.0.1",
    "react-mentions": "^4.4.10",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^25.3.1",
    "electron-builder": "^24.6.3",
    "typescript": "^5.3.2",
    "wait-on": "^8.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.yourcompany.siso",
    "files": [
      "app/build/**/*",
      "main.js",
      "main.py"
    ],
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}